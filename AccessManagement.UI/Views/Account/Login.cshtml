@using AccessManagement.UI.Models
@model LoginModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
</head>
<body>
    <form method="post">
        <p>UserName : <input type="text" name="UserName" value="@Model?.UserName" /></p>
        <p>Password : <input type="text" name="Password" value="@Model?.Password"/></p>
        <p><input type="submit" value="Login" /></p>
    </form>
</body>
</html>
